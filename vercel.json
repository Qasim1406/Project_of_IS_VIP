{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/debug",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/test",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/app-test",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
